    "@nx/js": "19.5.5",
    "@nx/plugin": "19.5.5",
    "@nx/vite": "19.5.5",
    "@nx/web": "19.5.5",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "~0.3.12",
    "@swc/core": "~1.5.7",
